# Example environment variables for MCP Census Server
# Copy this file to .env and fill in your own API keys

# Your US Census Bureau API key (required for census/get tool)
CENSUS_API_KEY=YOUR_CENSUS_API_KEY_HERE

# Your server API key for authenticating JSON-RPC calls
SERVER_API_KEY=YOUR_SERVER_API_KEY_HERE

# Optional: logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
#LOG_LEVEL=INFO